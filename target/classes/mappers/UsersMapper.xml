<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "mybatis-3-mapper.dtd" >
<mapper namespace="com.isscollege.user.dao.IUserDao" >
  <resultMap id="BaseResultMap" type="com.isscollege.user.entity.Users" >
    <id column="userid" property="userid" jdbcType="INTEGER" />
    <result column="username" property="username" jdbcType="VARCHAR" />
    <result column="password" property="password" jdbcType="VARCHAR" />
    <result column="sex" property="sex" jdbcType="INTEGER" />
  </resultMap>
  
  <select id="login" resultMap="BaseResultMap" parameterType="com.isscollege.user.entity.Users" >
    select * from users where username = #{username} and password=#{password}
  </select>
  
   <select id="getAllUsers" resultMap="BaseResultMap" useCache="true" statementType="PREPARED">
  	select * from users
  </select>
  
  
</mapper>